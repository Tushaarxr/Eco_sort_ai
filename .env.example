# E-Waste Assistant Environment Variables
# Copy this file to .env and fill in your values

# Gemini AI API Key (from Google AI Studio)
GEMINI_API_KEY=your_gemini_api_key_here

# Clerk Authentication (from Clerk Dashboard)
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_key_here

# Supabase Database (from Supabase Dashboard)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_KEY=your_supabase_anon_key_here
